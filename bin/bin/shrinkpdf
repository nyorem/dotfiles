#! /usr/bin/env bash
# Usage: input output [resolution]

in=$1

out=$2

res="300"
if [ "$#" -eq 3 ]; then
    res=$3
fi

gs -sDEVICE=pdfwrite -dCompatibilityLevel=1.4 -dPDFSETTINGS=/default \
    -dNOPAUSE -dQUIET -dBATCH -dDetectDuplicateImages \
    -dCompressFonts=true -r300 -sOutputFile=$out $in

